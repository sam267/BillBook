<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="easyway2in.com.billbook" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AppIntro"
            android:label="@string/title_activity_app_intro" >
        </activity>
        <activity
            android:name=".MainApp"
            android:label="@string/title_activity_app_main" >
        </activity>
        <activity
            android:name=".CreateProfile"
            android:label="@string/title_activity_create_profile" >
        </activity>
        <activity
            android:name=".ResetMonth"
            android:label="@string/title_activity_reset_month" >
        </activity>
        <activity
            android:name=".SignIn"
            android:label="@string/title_activity_sign_in" >
        </activity>
        <activity
            android:name=".Statistics"
            android:label="@string/title_activity_statistics" >
        </activity>
    </application>

</manifest>
